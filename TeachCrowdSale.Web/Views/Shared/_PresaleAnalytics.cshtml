<!-- Presale Analytics Section -->
<section class="presale-analytics-section analytics-section" id="presale">
    <div class="container">
        <div class="section-header">
            <h2 class="section-title">Presale Analytics</h2>
            <p class="section-subtitle">
                Comprehensive presale performance metrics, funding progress, and tier analysis
            </p>
        </div>

        <div class="presale-container">
            <!-- Presale Overview Metrics -->
            <div class="presale-overview">
                <div class="presale-metrics">
                    <div class="presale-metric primary">
                        <span class="metric-icon">💰</span>
                        <div class="metric-info">
                            <span class="metric-value" data-presale="totalRaised">$12.5M</span>
                            <span class="metric-label">Total Raised</span>
                        </div>
                        <div class="metric-change positive">
                            <span>+$2.1M</span>
                            <span>This Week</span>
                        </div>
                    </div>
                    <div class="presale-metric secondary">
                        <span class="metric-icon">🎯</span>
                        <div class="metric-info">
                            <span class="metric-value" data-presale="fundingProgress">14.3%</span>
                            <span class="metric-label">Funding Progress</span>
                        </div>
                        <div class="metric-change positive">
                            <span>Goal: $87.5M</span>
                        </div>
                    </div>
                    <div class="presale-metric success">
                        <span class="metric-icon">👥</span>
                        <div class="metric-info">
                            <span class="metric-value" data-presale="participants">2,847</span>
                            <span class="metric-label">Total Participants</span>
                        </div>
                        <div class="metric-change positive">
                            <span>+156</span>
                            <span>This Week</span>
                        </div>
                    </div>
                    <div class="presale-metric warning">
                        <span class="metric-icon">⚡</span>
                        <div class="metric-info">
                            <span class="metric-value" data-presale="velocity">$178K</span>
                            <span class="metric-label">Daily Average</span>
                        </div>
                        <div class="metric-change positive">
                            <span>+12%</span>
                            <span>vs Last Week</span>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Current Tier Analysis -->
            <div class="current-tier-analysis">
                <h3>Current Tier Performance</h3>
                <div class="tier-analysis-grid">
                    <div class="tier-info-card active-tier">
                        <div class="tier-header">
                            <h4>Community Round (Tier 2)</h4>
                            <span class="tier-price">$0.06 per TEACH</span>
                        </div>
                        <div class="tier-progress">
                            <div class="progress-bar">
                                <div class="progress-fill" data-percentage="45" style="width: 45%"></div>
                            </div>
                            <div class="progress-stats">
                                <span>169M / 375M tokens sold</span>
                                <span>45% Complete</span>
                            </div>
                        </div>
                        <div class="tier-metrics">
                            <div class="tier-metric">
                                <span class="metric-label">Sold Today</span>
                                <span class="metric-value">125K tokens</span>
                            </div>
                            <div class="tier-metric">
                                <span class="metric-label">Sales Velocity</span>
                                <span class="metric-value">1.25M/day</span>
                            </div>
                            <div class="tier-metric">
                                <span class="metric-label">Est. Sellout</span>
                                <span class="metric-value">165 days</span>
                            </div>
                        </div>
                    </div>

                    <div class="tier-performance-chart">
                        <h4>Tier Sales Performance</h4>
                        <div id="tierPerformanceChart" style="height: 300px;"></div>
                    </div>
                </div>
            </div>

            <!-- Presale Charts Grid -->
            <div class="presale-charts-grid">
                <div class="chart-container">
                    <h3 class="chart-title">Funding Progress Over Time</h3>
                    <div id="fundingProgressChart" style="height: 350px;"></div>
                </div>
                <div class="chart-container">
                    <h3 class="chart-title">Daily Investment Volume</h3>
                    <div id="dailyVolumeChart" style="height: 350px;"></div>
                </div>
                <div class="chart-container">
                    <h3 class="chart-title">Participant Growth</h3>
                    <div id="participantGrowthChart" style="height: 350px;"></div>
                </div>
                <div class="chart-container">
                    <h3 class="chart-title">Investment Distribution</h3>
                    <div id="investmentDistributionChart" style="height: 350px;"></div>
                </div>
            </div>

