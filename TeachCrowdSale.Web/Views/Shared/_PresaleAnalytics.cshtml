                        <div class="revenue-cell">$10.0M</div>
                        <div class="status-cell">
                            <span class="status-badge sold-out">Sold Out</span>
                        </div>
                    </div>

                    <div class="table-row active">
                        <div class="tier-cell">
                            <span class="tier-name">Community Round</span>
                            <span class="tier-number">Tier 2</span>
                        </div>
                        <div class="price-cell">$0.06</div>
                        <div class="allocation-cell">375M</div>
                        <div class="sold-cell">169M</div>
                        <div class="progress-cell">
                            <div class="mini-progress">
                                <div class="mini-progress-fill" style="width: 45%"></div>
                            </div>
                            <span>45%</span>
                        </div>
                        <div class="revenue-cell">$10.1M</div>
                        <div class="status-cell">
                            <span class="status-badge active">Active</span>
                        </div>
                    </div>

                    <div class="table-row upcoming">
                        <div class="tier-cell">
                            <span class="tier-name">Growth Round</span>
                            <span class="tier-number">Tier 3</span>
                        </div>
                        <div class="price-cell">$0.08</div>
                        <div class="allocation-cell">375M</div>
                        <div class="sold-cell">0</div>
                        <div class="progress-cell">
